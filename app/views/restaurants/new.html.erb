<div class="booyah-box col-xs-10 col-xs-offset-1">
	<h1> Add a new Restaurant </h1>

	<%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :restaurant_image %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
  <% end %>

	
</div>